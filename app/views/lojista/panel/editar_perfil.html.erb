<br/>
<%= form_with(model: @empresa_logado, url:{action: :editar_perfil} , local: true, html: { multipart: true }) do |form| %>
  <%= error_messages_for @empresa_logado %>
  <!-- Inicio Cabeçalho -->
  <div class="card mb-3">
    <!--/.bg-holder-->
  </div>
  <!-- Fim Cabeçalho -->
  <div class="card mb-3">
    <div class="card-header">
      <h3 class="mb-0"><%=  'Empresa Usuário'  %></h3>
      <h6 class="mb-1 float-right text-primary">Formulário</h6>
      <h3 class="mb-3 col-12 dropdown-divider"></h3>
    </div>
    <div class="card-body bg-light">
      <div class="row">
        <div class="col-12">
          <form>
            <div class="field">
              <%= form.label :empresa_id,("Empresa #{@empresa_logado.empresa.razao}"), class:' font-weight-extra-bold float-right text-primary', style:'font-size: 20px;' %>
            </div>
            <div class="form-group">
              <%= form.label :avatar, " Avatar" %>
              <%= form.file_field :avatar, class:'form-control' %>
            </div>
            <div class="form-group">
              <%= form.label :nome, "Nome" %>
              <%= form.text_field :nome, class:'form-control'%>
            </div>
            <div class="form-group">
              <%= form.label :email, "E-mail" %>
              <%= form.email_field :email, class:'form-control' %>
            </div>
            <div class="form-group">
              <%= form.label :senha, "Senha" %>
              <%= form.password_field :senha, class:'form-control' %>
            </div>
            <div class="form-group">
              <%= form.label :telefone, "Telefone" %>
              <%= form.number_field :telefone, class:'form-control' %>
            </div>
          </form>
          <%= form.submit 'Editar', class:'btn btn-primary mb-3'  %>
          <%= link_to 'Voltar', lojista_panel_visao_geral_path, class:'btn btn-warning mb-3' %>
        </div>
      </div>
    </div>
  </div>
<% end %>